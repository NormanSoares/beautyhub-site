{
  "version": 2,
  "name": "67-beauty-hub",
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "Imagens/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Produtos de beleza/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Produtos de conforto/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/aliexpress-callback",
      "dest": "/api/aliexpress-callback.js"
    },
    {
      "src": "/api/aliexpress-callback.php",
      "dest": "/api/aliexpress-callback.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "ALIEXPRESS_WEBHOOK_SECRET": "67beautyhub_webhook_secret_2024",
    "WEBHOOK_SECRET": "67beautyhub_webhook_secret_2024",
    "ALLOWED_ORIGINS": "*"
  },
  "installCommand": "npm install",
  "buildCommand": "echo 'Build completed'",
  "outputDirectory": "."
}

