{
  "version": 2,
  "name": "67-beauty-hub",
  "builds": [
    {
      "src": "api/**/*.php",
      "use": "@vercel/php"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "js/**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "css/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "Imagens/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Produtos de beleza/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "Produtos de conforto/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "ALIEXPRESS_WEBHOOK_SECRET": "67beautyhub_webhook_secret_2024"
  }
}



